<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ykx.ykxfmis.dao.BillDao">
    <resultMap id="BillMap" type="com.ykx.ykxfmis.entity.Bill" >
        <id property="id" column="b_id"></id>
        <result property="money" column="b_money"></result>
        <result property="datetime" column="b_date"></result>
        <result property="remark" column="b_remark"></result>
        <result property="id" column="u_id"></result>
    </resultMap>
<insert id="insertBillOne" parameterType="com.ykx.ykxfmis.entity.Bill" useGeneratedKeys="true" >
    insert into t_bill(b_money,b_date,b_remark,u_id) values(#{money},#{datetime},#{remark},#{id})
</insert>


</mapper>
